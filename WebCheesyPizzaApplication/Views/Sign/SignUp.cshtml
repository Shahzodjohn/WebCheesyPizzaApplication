@model WebCheesyPizzaApplication.ViewModels.SignUpViewModel
@{
    ViewData["Title"] = "SignUp";
}

<form action="SignUp" method="post">
    <input type="text" asp-for="Name" class="form-control" placeholder="First Name" />
    <input type="text" asp-for="LastName" class="form-control" placeholder="Last Name" />
    <input type="tel" asp-for="PhoneNumber" class="form-control" placeholder="Phone Number" />
    <input type="email" asp-for="Email"class="form-control" placeholder="Email" />
    <input type="text" asp-for="Login"class="form-control"  placeholder="Login" />
    <input type="password" asp-for="Password" placeholder="Password" />
    <input type="submit" value="Зарегистр" />
</form>